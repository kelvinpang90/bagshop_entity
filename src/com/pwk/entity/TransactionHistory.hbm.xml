<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.pwk.entity.TransactionHistory" table="transaction_history" catalog="bagshop">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="year" type="java.lang.String">
            <column name="year" length="5" />
        </property>
        <property name="month" type="java.lang.String">
            <column name="month" length="5" />
        </property>
        <property name="day" type="java.lang.String">
            <column name="day" length="5" />
        </property>
        <property name="amount" type="java.lang.Float">
            <column name="amount" />
        </property>
    </class>
</hibernate-mapping>
